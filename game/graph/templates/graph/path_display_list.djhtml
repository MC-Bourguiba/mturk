<ul style="list-style-type:none" id="path-list">
  {% for i in path_idxs %}
  <li>
    <a id="path-list-{{i}}" href='#'>Path {{i}}</a>
    <div style="float:left;">
      <input id="ex{{i}}" type="range" name="points" min="0" max="1" step="0.01" onchange="printValue('ex{{i}}', 'rangeValue{{i}}')">
    </div>
    <div style="display:inline-block;">
      <input id="rangeValue{{i}}" type="text" size="5" value="0.5">
    </div>
  </li>
  {% endfor %}
</ul>
